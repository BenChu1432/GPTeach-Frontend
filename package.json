{
    "name": "eng_ai_assistant",
    "version": "1.0.0",
    "main": "node_modules/expo/AppEntry.js",
    "scripts": {
        "start": "expo start",
        "start:dev": "env-cmd -f .env-cmdrc -e dev,dev expo start",
        "start:uat": "env-cmd -f .env-cmdrc -e uat,uat expo start",
        "start:prod": "env-cmd -f .env-cmdrc -e prod,prod expo start",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "build:android": "cd android && ./gradlew assembleRelease",
        "build:ios": "cd ios && xcodebuild -workspace YourApp.xcworkspace -scheme YourApp -configuration Release -sdk iphoneos -archivePath YourApp.xcarchive archive",
        "eas:build:android": "eas build --platform android",
        "eas:build:ios": "eas build --platform ios"
    },
